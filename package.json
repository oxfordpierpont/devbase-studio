{
  "name": "devbase-studio",
  "version": "0.1.0-alpha",
  "private": true,
  "description": "The WordPress of React and React Native - Visual development platform",
  "author": "Oxford Pierpont <oxford@oxfordpierpont.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oxfordpierpont/devbase-studio.git"
  },
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "db:migrate": "cd packages/database && npm run migrate",
    "db:seed": "cd packages/database && npm run seed"
  },
  "devDependencies": {
    "@turbo/gen": "^1.11.0",
    "turbo": "^1.11.0",
    "prettier": "^3.1.0",
    "eslint": "^8.54.0",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
